#!/usr/bin/env sh
# Compute the mean image from the imagenet training lmdb
# N.B. this is available in data/ilsvrc12

EXAMPLE=~/caffe-master/mycar
DATA=~/caffe-master/mycar
TOOLS=~/caffe-master/build/tools

$TOOLS/compute_image_mean $EXAMPLE/plate_train_lmdb \
  $DATA/plate_mean.binaryproto

echo "Done."
